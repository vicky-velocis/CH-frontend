            {
              "type": "length",
              "params": {
                "min": 0,
                "max": 250
              }
            }
          ]
        },
        {
          "path": "courtCases.advisorToAdminCourt",
          "required": false,
          "validations": [
            {
              "type": "length",
              "params": {
                "min": 0,
                "max": 250
              }
            }
          ]
        },
        {
          "path": "courtCases.honbleDistrictCourt",
          "required": false,
          "validations": [
            {
              "type": "length",
              "params": {
                "min": 0,
                "max": 250
              }
            }
          ]
        },
        {
          "path": "courtCases.honbleHighCourt",
          "required": false,
          "validations": [
            {
              "type": "length",
              "params": {
                "min": 0,
